{
  "comments": {
    "php": "// Automatic screenshot: Remove this comment if you want to manually change this file",
    "rst.txt": ".. Automatic screenshot: Remove this line if you want to manually change this file",
    "general": "Automatic screenshot:"
  },
  "extensions": [
    {
      "extension": "frontend",
      "paths": {
        "imageSource": "Documentation/Includes/Images/AutomaticScreenshots/",
        "relativeImagePath": "/Includes/Images/AutomaticScreenshots/",
        "imageRst": "Documentation/Includes/Images/RstIncludes/"
      },
      "modules": [
        {
          "module": "/module/web/ts",
          "screens": [
            {
              "filename": "TemplateAnalyzer",
              "caption": "All templates applying to a page, as used by the Introduction Package",
              "viewport": {"width": 960, "height": 640},
              "parameters": [
                {
                  "key": "id",
                  "value": 141
                },
                {
                  "key": "SET[function]",
                  "value": "TYPO3\\CMS\\Tstemplate\\Controller\\TemplateAnalyzerModuleFunctionController"
                }
              ]
            },
            {
              "filename": "TemplatesAnalyzerDetails",
              "caption": "Viewing the content of a given template in the Template Analyzer",
              "viewport": {"width": 960, "height": 640},
              "selector": "#TypoScriptTemplateModuleController",
              "parameters": [
                {
                  "key": "id",
                  "value": 141
                },
                {
                  "key": "template",
                  "value": "sys_4"
                }
              ]
            },
            {
              "filename": "TemplatesConstantEditor",
              "caption": "The Constant Editor showing some categories with constants",
              "viewport": {"width": 640, "height": 640},
              "parameters": [
                {
                  "key": "id",
                  "value": 141
                },
                {
                  "key": "SET[function]",
                  "value": "TYPO3\\CMS\\Tstemplate\\Controller\\TypoScriptTemplateConstantEditorModuleFunctionController"
                }
              ]
            },
            {
              "filename": "TemplatesConstants",
              "caption": "Overview of the defined constants",
              "viewport": {"width": 640, "height": 640},
              "selector": "#TypoScriptTemplateModuleController",
              "parameters": [
                {
                  "key": "id",
                  "value": 141
                },
                {
                  "key": "SET[function]",
                  "value": "TYPO3\\CMS\\Tstemplate\\Controller\\TypoScriptTemplateObjectBrowserModuleFunctionController"
                },
                {
                  "key": "SET[ts_browser_type]",
                  "value": "const"
                }
              ]
            },
            {
              "filename": "TemplatesSetup",
              "caption": "Overview of the defined setup",
              "viewport": {"width": 640, "height": 640},
              "selector": "#TypoScriptTemplateModuleController",
              "parameters": [
                {
                  "key": "id",
                  "value": 141
                },
                {
                  "key": "SET[function]",
                  "value": "TYPO3\\CMS\\Tstemplate\\Controller\\TypoScriptTemplateObjectBrowserModuleFunctionController"
                },
                {
                  "key": "SET[ts_browser_type]",
                  "value": "setup"
                },
                {
                  "key": "SET[ts_browser_const]",
                  "value": "subst"
                },
                {
                  "key": "tsbr[page]",
                  "value": 1
                }
              ]
            }
          ]
        }
      ],
      "tables": [
        {
          "table": "sys_template",
          "screens": [
            {
              "name": "TemplatesIncludes",
              "view": "record",
              "uid": 2,
              "actions": [
                {
                  "action": "click",
                  "tab": "Includes"
                },
                {
                  "action": "wait",
                  "timeout": "500"
                }
              ],
              "selector": "[role=\"tabpanel\"]"
            }
          ]
        }
      ]
    }
  ]
}